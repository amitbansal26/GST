CREATE TABLE IF NOT EXISTS `gst-default`.`g_gst_rate` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `item_code` BIGINT(16) NULL,
  `item_desc` VARCHAR(255) NULL,
  `sgst_rate` DECIMAL(6,2) NULL,
  `cgst_rate` DECIMAL(6,2) NULL,
  `igst_rate` DECIMAL(6,2) NULL,
  `cess_rate` DECIMAL(6,2) NULL,
  PRIMARY KEY (`id`));
